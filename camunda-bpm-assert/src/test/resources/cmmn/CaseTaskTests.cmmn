  <cmmn:case id="Case_CaseTaskTests" name="CaseTask Tests">
    <cmmn:casePlanModel autoComplete="false" description="" id="CPM_CaseTaskTests" name="CaseTask Tests">

      <cmmn:planItem definitionRef="CT_A" id="PI_CT_A"/>
      <cmmn:planItem definitionRef="CT_B" entryCriteriaRefs="On_PI_CT_A_Complete" id="PI_CT_B"/>
      <cmmn:planItem definitionRef="CT_C" id="PI_CT_C"/>

      <cmmn:sentry id="On_PI_CT_A_Complete">
