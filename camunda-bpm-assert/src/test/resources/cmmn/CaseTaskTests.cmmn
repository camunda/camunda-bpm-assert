          <cmmn:standardEvent>complete</cmmn:standardEvent>
        </cmmn:planItemOnPart>
      </cmmn:sentry>

      <cmmn:caseTask description="" id="CT_A" isBlocking="true" name="A" caseRef="dummyCase">
        <cmmn:defaultControl>
          <cmmn:manualActivationRule id="MAR_CT_A">
            <cmmn:condition id="Cond_MAR_CT_A" language="juel">
              <cmmn:body>#{false}</cmmn:body>
            </cmmn:condition>
          </cmmn:manualActivationRule>
        </cmmn:defaultControl>
      </cmmn:caseTask>

      <cmmn:caseTask description="" id="CT_B" isBlocking="true" name="B" caseRef="dummyCase">
        <cmmn:defaultControl>
          <cmmn:manualActivationRule id="MAR_CT_B">
            <cmmn:condition id="Cond_MAR_CT_B" language="juel">
              <cmmn:body>#{false}</cmmn:body>
            </cmmn:condition>
          </cmmn:manualActivationRule>
        </cmmn:defaultControl>
      </cmmn:caseTask>

      <cmmn:caseTask description="" id="CT_C" isBlocking="true" name="C" caseRef="dummyCase">
        <cmmn:defaultControl>
          <cmmn:manualActivationRule id="MAR_CT_c">
            <cmmn:condition id="Cond_MAR_CT_C" language="juel">
              <cmmn:body>#{true}</cmmn:body>
            </cmmn:condition>
          </cmmn:manualActivationRule>
        </cmmn:defaultControl>
      </cmmn:caseTask>
    </cmmn:casePlanModel>
  </cmmn:case>

  <cmmn:case id="dummyCase">
    <cmmn:casePlanModel>

      <cmmn:planItem id="foobar" definitionRef="foo">
        <cmmn:itemControl>
          <cmmn:requiredRule>
            <cmmn:condition>
              <cmmn:body>${true}</cmmn:body>
            </cmmn:condition>
          </cmmn:requiredRule>
        </cmmn:itemControl>
      </cmmn:planItem>
      <cmmn:humanTask id="foo" isBlocking="true">
        <cmmn:defaultControl>
          <cmmn:manualActivationRule>
            <cmmn:condition>
              <cmmn:body>${false}</cmmn:body>
            </cmmn:condition>
          </cmmn:manualActivationRule>
        </cmmn:defaultControl>
      </cmmn:humanTask>

    </cmmn:casePlanModel>
  </cmmn:case>

</cmmn:definitions>

